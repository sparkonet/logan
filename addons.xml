<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.audio.jango"
       name="Jango"
       version="0.9.3"
       provider-name="anonymous">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.t0mm0.common" version="1.1.0"/>
        <import addon="script.module.requests" version="1.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               	library="default.py">
        <provides>audio</provides>
    </extension>
	<extension point="xbmc.service" library="playerMP3.py" start="startup" />
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>Free Internet Radio that plays music you'll love!</summary>
        <description>Free internet radio. Listen to hundreds of genre stations or create your own with your favorite music.</description>
    </extension>
</addon>

<addon id="plugin.program.advancedsettings" name="Easy Advanced Settings" version="0.8.9" provider-name="kinkin">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>For Kodi 16: Create, view, edit and delete your advancedsettings.xml file in a user-friendly gui</summary>
        <description>All settings follow the xbmc.org wiki[CR][CR]With this tool you can create your advanced settings directly from XBMC[CR][CR]If you don't know what a setting does, don't enable it!</description>
        <platform>all</platform>
        <language>en</language>
        <license>GNU GENERAL PUBLIC LICENSE</license>
        <forum></forum>
        <website></website>
        <source></source>
        <email></email>
    </extension>
</addon>

<addon id="repository.logan" name="Logan" version="2.5" provider-name="Xiver">
    <extension point="xbmc.addon.repository" name=" Logan REPOSITORY">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/sparkonet/logan/master/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/sparkonet/logan/master/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/sparkonet/logan/master/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Logan</summary>
        <description>Logan</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>

<addon id="script.subskeys"
       name="מקשים לכתוביות"
       version="1.0.1"
       provider-name="Subs">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.pyxbmct" version="1.1.4"/>

  </requires>
	<extension point="xbmc.python.script" library="default.py">
		<provides>executable</provides>
	</extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Keyboard map for subtitles</summary>
    <language>en</language>
    <description lang="en">Keyboard map for subtitles</description>
     <platform>all</platform>
  </extension>
</addon>
</addons>
